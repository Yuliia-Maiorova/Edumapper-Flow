  "dependencies": {
    "nuxt": "^3.19.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@tailwindcss/postcss": "^4.1.14",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "vue-tsc": "^2.2.12"
